            <div id="Area" class="tab-pane ">
                <div class="container">
                    <div class="row sticky">
                        <div class="col-sm-7 col-md-7 col-lg-7 col-xl-7">
                            <div class="row">
                                <div class="col col-md-6 col-12">
                                    <select class="form-control" name="calc_shipping_provinces" id="calc_shipping_provinces" required="" onchange="loadListDistrict(this.value)">
                                        <option value="">Tỉnh / Thành phố</option>
                                        @foreach (Data.DB.Province obj in ViewBag.Province)
                                        {
                                            <option value="@obj.ID">
                                                @obj.Name
                                            </option>
                                        }
                                    </select>
                                </div>
                                <div class="col col-md-6 col-12">
                                    <select class=" form-control" name="calc_shipping_district" id="slDistrict" required="">
                                        <option value="">Quận / Huyện</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-5 col-md-5 col-lg-5 col-xl-5 text-right">
                            <button class="btn btn-primary" onclick="javaScript:searchArea()"><i class="fa fa-search mr-1"></i>Tìm kiếm</button>
                            <button class="btn btn-success" data-toggle="modal" data-target="#mdAddArea"><i class="fa fa-plus mr-1"></i>Thêm mới</button>
                        </div>
                    </div>

                    <div class="row mt-3" id="tblSearchArea">
                        @{
                            Html.RenderAction("SearchActiveArea", "Config", new { page = 1 });
                        }

                    </div>
                </div>


            </div>

            @* View hoa hồng *@
            <div id="Commission" class="tab-pane">
                <div class="container">
                    <div class="row sticky">
                        <div class="col-sm-4 col-md-4 col-xl-4 col-lg-4">
                            <div class="row">

                                <div class="col-sm-10 col-md-10 col-xl-10 col-lg-10">
                                    <input type="text" id="nameCommission" class="form-control" placeholder="Tên mức hoa hồng" />
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-8 col-md-8 col-xl-8 col-lg-8 text-right">
                            <button class="btn btn-primary mr-1" id="btnSearchServiArea" onclick="SearchCommission()"><i class="fa fa-search mr-1"></i>Tìm kiếm</button>
                            <button class="btn btn-success" data-toggle="modal" data-target="#mdAddCommnisisson"><i class="fa fa-plus mr-1"></i>Thêm mới</button>
                        </div>
                    </div>

                    <div class="row mt-3" id="tblSearchCommission">
                        @{
                            Html.RenderAction("SearchConfigCommission", "Config", new { page = 1 });
                        }

                    </div>
                </div>
            </div>

            <div id="QA" class="tab-pane">
