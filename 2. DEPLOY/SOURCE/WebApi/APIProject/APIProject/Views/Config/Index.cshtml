                        <fieldset class="scheduler-border">
                            <legend class="scheduler-border"><label class="text-primary"><h4>Ví</h4></label></legend>
                            <form id="frmWallet" enctype="multipart/form-data">
                                <div class="row">
                                    <div class="col-sm-6 col-md-6 col-lg-6 col-xl-6">
                                        <label class="text-dark">
                                            Ví cọc tối thiểu:
                                        </label>
                                    </div>
                                    <div class="col-sm-6 col-md-6 col-lg-6 col xl-6">
                                        <input type="hidden" name="ConfigWalletNoWithdrawID" value="" />
                                        <input type="text" class="form-control number" name="ConfigWalletNoWithdrawValue" id="txtMinimunBalanceDesposit" value="@string.Format("{0:#,0}", Convert.ToDecimal( @dt.WalletConfig.ConfigWalletNoWithdrawValue))" />
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col-sm-6 col-md-6 col-lg-6 col-xl-6">
                                        <label class="text-dark">
                                            Ví thu nhập tối thiểu:
                                        </label>
                                    </div>
                                    <div class="col-sm-6 col-md-6 col-lg-6 col xl-6">
                                        <input type="hidden" name="ConfigWalletWithdrawID" value="@dt.WalletConfig.ConfigWalletWithdrawID" />
                                        <input class="form-control number" name="ConfigWalletWithdrawValue" type="text" id="txtMinimunBalanceCash" value="@string.Format("{0:#,0}", Convert.ToDecimal( @dt.WalletConfig.ConfigWalletWithdrawValue))" />
                                    </div>
                                </div>
                                <div class="row mt-3">
                                    <div class="col-sm-6 col-md-6 col-lg-6 col-xl-6">
                                        <label class="text-dark">
                                            Thời gian rút tiền:
                                        </label>
                                    </div>
                                    <div class="col-sm-3 col-md-3 col-lg-3 col">
                                        <input type="hidden" name="ConfigTimeWithdrawID" value="@dt.WalletConfig.ConfigTimeWithdrawID" />
                                        <input type="number" class="form-control number-type" name="ConfigTimeWithdrawValue" id="txtMinimunBalanceSendMess" value="@dt.WalletConfig.ConfigTimeWithdrawValue" />
                                    </div>
                                    <div class="col-sm-2 col-md-3 col-lg-3 col mt-10">
                                        <label>Ngày</label>
                                    </div>
                                </div>
                            </form>
                            <div class="row mt-5">
                                <div class="col-sm-2 col-md-2 col-lg-2 col-xl-2 offset-9 text-right">
                                    <button class="btn btn-success" onclick="saveWallet()"><i class="fa fa-save mr-1"></i>Lưu</button>
                                </div>
                            </div>
                        </fieldset>
                    </div>
                    <div class="col-ms-6 col-md-6 col-lg-6 col-xl-6">
                        <fieldset class="scheduler-border">
                            <legend class="scheduler-border"><label class="text-primary"><h4>Giao dịch</h4></label></legend>
                            <form id="frmtransaction" enctype="multipart/form-data">
                                <div class="row">
                                    <div class="col-sm-7 col-md-7 col-lg-7 col-xl-4">
                                        <label class="text-dark">
                                            Thời gian nhận
                                        </label>
                                    </div>
                                    <div class="col-sm-5 col-md-5 col-lg-5 col-xl-4">
                                        <div class="row">
                                            <div class="col-md-9 col-sm-9 col-xs-9">
                                                <input type="hidden" id="CountDownID" name="CountDownConfigID" value="@dt.TransactionConfig.CountDownConfigID" />
                                                <input type="number" class="form-control number-type" id="countdownconfig" name="CountDownConfigValue" value="@dt.TransactionConfig.CountDownConfigValue" />
                                            </div>
                                            <div class="col-md-3 col-sm-3 col-xs-3  mt-10"><label class="text-dark">Giây</label></div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row mt-3">
                                    <div class="col-sm-7 col-md-7 col-lg-4 col-xl-4"><label class="text-dark">Hủy giao dịch:</label></div>
                                    <div class="col-sm-5 col-md-5 col-lg-4 col-xl-4">
                                        <div class="row ">
                                            <input type="hidden" id="CancelTimeID" name="CancelTimeConfigID" value="@dt.TransactionConfig.CancelTimeConfigID" />
                                            <div class="col-md-9 col-sm-9 col-xs-9">
                                                <input type="number" class="form-control number-type" id="cancelTransaction" name="CancelTimeConfigValue" value="@dt.TransactionConfig.CancelTimeConfigValue" />
                                            </div>
                                            <div class="col-md-3 col-sm-3 col-xs-3  mt-10"><label class="text-dark">Lần</label></div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row mt-3">
                                    <div class="col-sm-7 col-md-7 col-lg-7 col-xl-4">
                                        <label class="text-dark">
                                            Thời gian hủy hóa đơn
                                        </label>
                                    </div>
                                    <div class="col-sm-5 col-md-5 col-lg-5 col-xl-4">
                                        <div class="row">
                                            <div class="col-md-9 col-sm-9 col-xs-9">
                                                <input type="hidden" id="TimeCancelID" name="TimeCancelOrderID" value="@dt.TransactionConfig.TimeCancelOrderID" />
                                                <input type="number" class="form-control number-type" id="TimeCancelValue" name="TimeCancelOrderValue" value="@dt.TransactionConfig.TimeCancelOrderValue" />
                                            </div>
                                            <div class="col-md-3 col-sm-3 col-xs-3  mt-10"><label class="text-dark">Giây</label></div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row mt-3">
                                    <div class="col-sm-7 col-md-7 col-lg-4 col-xl-4">
                                        <label class="text-dark">
                                            Phí thu hộ tối đa:
                                        </label>
                                    </div>
                                    <div class="col-sm-5 col-md-5 col-lg-8 col-xl-8">
                                        <div class="row">
                                            <input type="hidden" id="MaxCODID" name="MaxCODConfigID" value="@dt.TransactionConfig.MaxCODConfigID" />
                                            <div class="col-md-9"><input type="text" class="form-control number" id="maxcod" name="MaxCODConfigValue" value="@string.Format("{0:#,0}", Convert.ToDecimal(@dt.TransactionConfig.MaxCODConfigValue))" /></div>
                                            <div class="col-md-3"><label class="text-dark">VNĐ</label></div>

                                        </div>
                                    </div>
                                </div>
                            </form>
                            <div class="row mt-5">
                                <div class="col-sm-2 col-md-2 col-lg-2 col-xl-2 text-right offset-9">
                                    <button class="btn btn-success" onclick="saveUpdateTransactionConfig()"><i class="fa fa-save mr-1"></i>Lưu</button>
                                </div>
                            </div>
                        </fieldset>
                    </div>
                </div>

                <div class="row mt-3">
                    <div class="col-ms-6 col-md-6 col-lg-6 col-xl-6">
                        <fieldset class="scheduler-border">
                            <legend class="scheduler-border"><label class="text-primary"><h4>Khu vực hoạt động</h4></label></legend>
                            <form id="fromGPS">
                                <div class="row mt-3">
                                    <div class="col-md-6 col-sm-8 col-lg-6 mt-6 col-xs-8">
                                        <label class="text-dark">
                                            Khu vực hoạt động tối đa:
                                        </label>
                                    </div>
                                    <div class="col-md-4 col-sm-4 col-lg-4 col-xs-4">
                                        <input type="hidden" name="MaxAreaConfigID" value="@dt.AreaConfig.MaxAreaConfigID" />
                                        <input name="MaxAreaConfigValue" id="valMaximumArea" class="form-control number" type="text" value="@dt.AreaConfig.MaxAreaConfigValue" />
                                    </div>
                                </div>
                            </form>
                            <div class="row mt-5">
                                <div class="col-sm-2 col-md-2 col-lg-2 col-xl-2 text-right offset-9">
                                    <button class="btn btn-success" onclick="saveUpdateAreaConfig()"><i class="fa fa-save mr-1"></i>Lưu</button>
                                </div>
                            </div>
                        </fieldset>
                    </div>

                    <div class="col-ms-6 col-md-6 col-lg-6 col-xl-6">
                        <fieldset class="scheduler-border">
                            <legend class="scheduler-border"><label class="text-primary"><h4>Khung giờ cao điểm</h4></label></legend>
                            <div class="row mt-2 ml-1">
                                <button data-toggle="modal" data-target="#mdAddRushHours" class="btn btn-primary mr-10"><i class="fa fa-plus mr-1"></i>Thêm</button>
                                <button class="btn btn-success" onclick="saveUpdateConfigTime()"><i class="fa fa-save mr-1"></i>Lưu</button>
                            </div>
                            <div class="row mt-2 ml-1">
                                <form id="frm-update-config-time" enctype="multipart/form-data">
                                    <table class="table table-light table-bordered table-hover table-responsive-sm table-responsive-md">
                                        <thead class="text-center text-white bg-thead">
                                            <tr>
                                                @*<th>STT</th>*@
                                                <th>Khung giờ</th>
                                                <th>Phí tăng trong giờ cao  điểm</th>
                                                <th>Mô tả</th>
                                                <th></th>
                                            </tr>

                                        </thead>
                                        <tbody class="text-center" id="configtime-table">
                                            @{
                                                //int stt = 1;
                                                foreach (var i in dt.PeakHourConfig)
                                                {
                                                    <tr class="val-configtime-id" data-id="@i.ID" data-startTime="@i.StartTime" data-endTime="@i.EndTime" data-Price="@i.Price" data-Desc="@i.Description">
                                                        <td>
                                                            <input class="conf-time-input start-time" type="text" value="@i.StartTime" /> -
                                                            <input class="conf-time-input end-time" type="text" value="@i.EndTime" />
                                                        </td>
                                                        <td><input class="conf-time-input-w price number" type="text" value="@string.Format("{0:#,0}", Convert.ToDecimal(@i.Price))" />VND/km</td>
                                                        <td><input class="conf-time-input-w desc" type="text" value="@i.Description" /></td>
                                                        <td>
                                                            <a onclick="deleteRow(this)" style="color: red; cursor: pointer"><i class="fa fa-fw fa-trash"></i></a>
                                                        </td>
                                                    </tr>
                                                    //stt++;
                                                }
                                            }


                                        </tbody>
                                    </table>
                                </form>
                            </div>

                        </fieldset>
                    </div>
                </div>

            </div>

