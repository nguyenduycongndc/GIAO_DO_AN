    //Tìm kiếm Hoa hồng
    function SearchCommission() {
        var _nameCommission = $('#nameCommission').val();
        $.ajax({
            url: '/Config/SearchConfigCommission',
            data: {
                page: 1,
                nameCommission: _nameCommission,
            },
            success: function (response) {
                $('#tblSearchCommission').html(response);
            },
            error: function (result) {
                console.log(result.responseText);
            }
        })
    }
    //xóa hoa hồng
    function DeleteConfigCommission(id) {
        swal({
            title: "Bạn chắc chắn muốn xóa bản ghi này không?",
            text: "",
            icon: "warning",
            buttons: ["Cancel", "OK"],
            dangerMode: true,
        }).then((willDelete) => {
            if (willDelete) {
                $.ajax({
                    url: '/Config/DeleteConfigCommission',
                    type: 'POST',
                    data: { id: id },
                    success: function (res) {
                        if (res) {
                            swal({
                                title: "",
                                text: "Xóa bản ghi thành công",
                                icon: "success"
                            });
                            SearchCommission();
                        }
                        else {
                            swal({
                                title: "",
                                text: "Xóa bản ghi thất bại",
                                icon: "error"
                            })
                        }
                    }
                })
            }
        })
    }
    //Lưu khi thêm mới hoa hồng
    function saveConfigCommission() {
        var _createComissionName = $("#createComissionName").val();
        var _createComissionValue = $("#createComissionValue").val();
        if (_createComissionName == "" || _createComissionValue == "") {
            swal({
                title: "",
                text: "Hãy nhập đủ thông tin",
                icon: "warning"
            });
            return;
        }
        if (_createComissionValue < 0 || _createComissionValue > 100) {
            swal({
                title: "",
                text: "Giá trị hoa hồng phải có giá trị từ 0 đến 100",
                icon: "error"
            });
            return;
        }
        $.ajax({
            url: '/Config/CreateConfigCommission',
            type: 'POST',
            data: {
                commissionName: _createComissionName,
                mastersBenefit: _createComissionValue,
            },
            success: function (res) {
                if (res == 1) {
                    swal({
                        title: "",
                        text: "Thêm mới thành công",
                        icon: "success"
                    });
                    $("#mdAddArea").modal('hide');
                    $('.modal-backdrop').remove();
                    SearchCommission();
                }
                else {
                    swal({
                        title: "",
                        text: "Thêm mới thất bại",
                        icon: "error"
                    })
                }
            }
        })
    }
    //Binding dữ liệu hoa hồng
    function EditConfigCommission(id) {

        $.ajax({
            url: '/Config/GetConfigCommissions',
            type: 'GET',
            data: {
                id: id,
            },
            success: function (res) {
                $("#txtConfigComissionID").val(res.ID);
                $("#editComissionName").val(res.CommissionName);
                $("#editComissionValue").val(res.MastersBenefit);
                $("#mdEditCommnisisson").modal('show');
            }
        })
    }
    //Lưu khi sửa hoa hồng
    function saveEditConfigCommission() {
        var _id = $("#txtConfigComissionID").val();
        var _name = $("#editComissionName").val();
        var _mastersBenefit = $("#editComissionValue").val();
        var _isActive = 1;
        if (_name == "" || _mastersBenefit == "") {
            swal({
                title: "",
                text: "Hãy nhập đủ thông tin",
                icon: "warning"
            });
            return;
        }
        if (_mastersBenefit < 0 || _mastersBenefit > 100) {
            swal({
                title: "",
                text: "Giá trị hoa hồng phải có giá trị từ 0 đến 100",
                icon: "error"
            });
            return;
        }
        $.ajax({
            url: '/Config/SaveEditConfigCommission',
            type: 'POST',
            data: {
                id: _id,
                CommissionName: _name,
                mastersBenefit: _mastersBenefit,
                IsActive: _isActive,
            },
            success: function (res) {
                if (res) {
                    swal({
                        title: "",
                        text: "Lưu thành công",
                        icon: "success"
                    });
                    SearchCommission();
                    $("#mdEditCommnisisson").modal('hide');
                    $('.modal-backdrop').remove();
                }
                else {
                    swal({
                        title: "",
                        text: "Lưu thất bại",
                        icon: "error"
                    })
                    $("#mdEditCommnisisson").modal('hide');
                    $('.modal-backdrop').remove();
                }
            }
        })
    }
    //clear form khi thêm mới
    function btnAddVehicleType() {
        $('#addName').val('');
        $('#addOrder').val('');
        $("#AddLogoLicense").html('<i class="fa fa-camera" style="font-size:30px;"></i>');

    }

    function AddConfigTime() {

        var start_time = $("#conf_starttime").val();
        var end_time = $("#conf_endtime").val();
        var price = $("#conf_price").val();
        var desc = $("#conf_desc").val();
        if (start_time == "" || end_time == "" || price == "" || desc == "") {
            swal({
                title: "Vui lòng nhập đầy đủ thông tin!",
                icon: 'warning'
            })
            return;
        }
        var ckeckExistingArea = 0
        $.each($('.val-configtime-id'), function () {
            if ($(this).attr('data-startTime') == start_time && $(this).attr('data-endTime') == end_time) {
                ckeckExistingArea = 1;
            }
        })
        if (ckeckExistingArea > 0) {
            swal({
                title: "Giờ cao điểm đã được chọn trước đó!",
                icon: 'warning'
            })
            return;
        }
        $("#mdAddRushHours").modal('hide');
        var row =
            '<tr class = "val-configtime-id"  data-startTime = "' + start_time + '" data-endTime = "' + end_time + '" data-Price = "' + price + '" data-Desc = "' + desc + '">' +
            '<td>' +
            '<input class="conf-time-input start-time" type="text" value="' + start_time + '" /> -' +
            '<input class="conf-time-input end-time" type="text" value="' + end_time + '" />' +
            '</td>' +
            '<td>' +
            '<input class="conf-time-input-w price number" type="text" value="' + price + '" />' + 'VND/km' +
            '</td>' +
            '<td>' +
            '<input class="conf-time-input-w desc" type="text" value="' + desc + '" />' +
            '</td>' +
            '<td>' +
            '<a onclick="deleteRow(this)" style="color: red; cursor: pointer;"><i class="fa fa-fw fa-trash"></i></a>' +
            '</td>' +
            '</tr >';
        $("#configtime-table").append(row);

    }

    function deleteRow(del) {
        $(del).closest('tr').remove();
    }

</script>
