                        <button class="btn btn-success mr-1" data-toggle="modal" data-target="#mdAddTypeVehicle" onclick="javaScript:btnAddVehicleType()"><i class="fa fa-plus mr-1"></i>Thêm loại xe</button>
                    </div>
                </div>

                <div class="row mt-3">
                    <div class="col-md-12" id="ListVehicleType">
                        @{
                            Html.RenderAction("SearchVehicleType", "Config", new { page = 1 });
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
