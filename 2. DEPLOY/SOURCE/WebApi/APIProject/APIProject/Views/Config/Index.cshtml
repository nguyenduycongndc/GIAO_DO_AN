            <div id="WorkingTime" class="tab-pane ">
                <div class="container">
                    <div class="row sticky">
                        <div class="col col-md-8 col-12">
                            <div class="row">
                                <div class="col col-6">
                                    <select class="form-control" id="slcType">
                                        <option>Loại dịch vụ</option>
                                        <option value="@SystemParam.SHIP_DRIVER">@SystemParam.SHIP_DRIVER_STR</option>
                                        <option value="@SystemParam.SHIP_PACKAGE">@SystemParam.SHIP_PACKAGE_STR</option>
                                        <option value="@SystemParam.SHIP_FOOD">@SystemParam.SHIP_FOOD_STR</option>
                                    </select>
                                </div>
                                <div class="col col-6">
                                    <select class="form-control" id="slcTypeVehice">
                                        <option>Chọn loại xe</option>
                                        @foreach (Data.DB.VehicleType obj in ViewBag.VehicleType)
                                        {
                                            <option value="@obj.ID">@obj.Name @*- @obj.Logo*@</option>
                                        }
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col col-md-4 col-12 text-right">
                            <button onclick="SearchConfigTransportCost()" class="btn btn-primary"><i class="fa fa-search mr-1"></i>Tìm kiếm</button>
                            <button onclick="loadConfigDriver()" class="btn btn-success" data-toggle="modal" data-target="#mdAddServicePrice1"><i class="fa fa-plus mr-1"></i>Thêm mới</button>
                        </div>
                    </div>

                    <div class="row mt-3" id="_TableTransportCost">
                        @{ Html.RenderAction("SearchConfigTranportCost", "Config", new { Page = 1 });}
                    </div>
                </div>

            </div>

            @*Gía dịch vụ chuyển phát nhanh*@
