{
  "name": "Washer",
  "version": "1.0",
  "private": true,
  "scripts": {
    "8081": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9090 tcp:9090 && yarn start",
    "8082": "adb reverse tcp:8082 tcp:8082 && adb reverse tcp:9090 tcp:9090 && yarn start --port 8082",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "li": "node generateAssetResource.js",
    "iosdevice": "react-native run-ios --device \"6S\"",
    "lint": "eslint .",
    "reverse": "adb reverse tcp:9090 tcp:9090; adb reverse tcp:19001 tcp:19001;",
    "adb": "adb reverse tcp:9090 tcp:9090; adb reverse tcp:19001 tcp:19001;",
    "adb-start": "emulator @A",
    "adbreload": "adb shell input keyevent KEYCODE_MENU",
    "pod": "cd ios && pod install && cd ..",
    "ba": "cd android && ./gradlew assembleRelease --warning-mode all"
  },
